	"version": "0.2.0",

	"configurations": [
		{
			"command": "npm run dev",
			"name": "Dev Tubesleuth",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}"
		},
		{
			"command": "npm run video",
			"name": "Video Tubesleuth",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "Attach by Process ID",
			"processId": "${command:PickProcess}",
			"request": "attach",
			"skipFiles": ["<node_internals>/**"],
			"type": "node"
		}
	]
}
